  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1.0.13",
    "@std/yaml": "jsr:@std/yaml@1.0.8",
    "ansi-colors": "npm:ansi-colors@4.1.3",
    "lionweb-core": "npm:@lionweb/core@0.7.1",
    "lionweb-json": "npm:@lionweb/json@0.7.1",
    "lionweb-utilities": "npm:@lionweb/utilities@0.7.1"
  },
  "nodeModulesDir": "auto"
}
